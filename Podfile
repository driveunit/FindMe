# Uncomment the next line to define a global platform for your project
 platform :ios, '11.0'

target 'Find_Me' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Find_Me
  target :'Find_Me' do
    pod 'AWSAuth'
    pod 'AWSAuthCore'
    pod 'AWSAuthUI'
    pod 'AWSCognito'
    pod 'AWSCognitoAuth'
    pod 'AWSDynamoDB'
    pod 'AWSEC2'
    pod 'AWSFacebookSignIn'
    pod 'AWSGoogleSignIn'
    pod 'AWSLex'
    pod 'AWSLogs'
    pod 'AWSMobileClient'
    pod 'AWSPinpoint'
    pod 'AWSPolly'
    pod 'AWSRekognition'
    pod 'AWSS3'
    pod 'AWSUserPoolsSignIn'
    pod 'AWSCognitoIdentityProvider'
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'Async'
    pod 'PKHUD'

  end
  target 'Find_MeTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Find_MeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
